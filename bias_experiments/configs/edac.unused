command:
  - python3
  - ${program}
  - ${args_no_boolean_flags}
entity: flair
method: random
name: EDAC
program: algorithms/edac.py
project: unifloral

parameters:
  # --- Experiment ---
  seed:
    values: [0, 1]
  dataset:
    values: [square-reach/horizon-10-v0, square-reach/horizon-20-v0, square-reach/horizon-50-v0,
             pendulum/gaussian-v0, pendulum/uniform-v0, pendulum/mixture-v0]
  algorithm:
    value: cql
  num_updates:
    value: 1_000_000
  eval_interval:
    value: 2500
  eval_workers:
    value: 1
  eval_final_episodes:
    value: 1000

  # --- Logging ---
  log:
    value: true
  wandb_project:
    value: square-edac
  wandb_team:
    value: ahoj
  wandb_group:
    value: debug

  # --- Generic optimization ---
  lr:
    value: 0.0003
  batch_size:
    value: 256
  gamma:
    value: 0.999
  polyak_step_size:
    value: 0.005

  # --- SAC-N ---
  num_critics:
    values: [10, 20, 50]  # 100 has high GPU memory usage

  # --- EDAC ---
  eta:
    values: [0.0, 1.0, 5.0, 10.0, 100.0, 200.0, 500.0, 1000.0]
